{
  "name": "solo-nestjs-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:matching": "jest --config ./test/jest-e2e.json test/matching/matching.e2e-spec.ts",
    "seed": "ts-node -r dotenv/config -r tsconfig-paths/register src/console.ts seed:users",
    "seed:clear": "ts-node -r dotenv/config -r tsconfig-paths/register src/console.ts seed:users --clear",
    "seed:articles": "ts-node -r dotenv/config -r tsconfig-paths/register src/console.ts seed:articles",
    "seed:articles:clear": "ts-node -r dotenv/config -r tsconfig-paths/register src/console.ts seed:articles --clear",
    "embedding:interest": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/console.ts generate:embeddings",
    "embedding:interest:user": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/console.ts generate:embeddings -u",
    "embedding:interest:option": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/console.ts generate:embeddings -o",
    "embedding:profile": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/console.ts generate:profile-embeddings",
    "embedding:profile:user": "node -r dotenv/config -r ts-node/register -r tsconfig-paths/register src/console.ts generate:profile-embeddings -u",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "migration:run": "ts-node -r dotenv/config -r tsconfig-paths/register src/database/run-migrations.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@keyv/redis": "^4.3.3",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.1",
    "@portone/server-sdk": "^0.14.0",
    "@qdrant/qdrant-js": "^1.13.0",
    "@slack/web-api": "^7.9.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.2",
    "cache-manager-redis-store": "^3.0.1",
    "cacheable": "^1.8.10",
    "ioredis": "^5.3.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dayjs-plugin-utc": "^0.1.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.2",
    "nest-commander": "^3.17.0",
    "nodemailer": "^6.10.1",
    "openai": "^4.91.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.6.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "coolsms-node-sdk": "^2.1.0",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@auth/(.*)$": "<rootDir>/src/auth/$1",
      "^@common/(.*)$": "<rootDir>/src/common/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@database/(.*)$": "<rootDir>/src/database/$1"
    }
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "bcrypt"
    ]
  }
}